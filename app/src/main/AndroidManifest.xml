<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tuse1.sensorapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SensorListActivity"
        android:label="SensorList">
    </activity>
        <activity android:name=".LightSensorActivity"
            android:label="LightSensor">
        </activity>


    <activity android:name=".ProximitySensorActivity"
        android:label="ProximitySensor">
    </activity>

        <activity android:name=".AccelerometerActivity"
            android:label="AccelerometerSensor">
        </activity>

        <service android:name=".MyService" />

    </application>

</manifest>